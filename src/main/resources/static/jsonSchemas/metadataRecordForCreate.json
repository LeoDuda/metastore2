{
    "type": "object",
    "properties": {
        "id": {
            "type": "string",
            "title": "Identifier"
        },
        "relatedResource": {
            "type": "string",
            "title": "Related Resource"
        },
        "schemaId": {
            "type": "string",
            "title": "Schema Identifier"
        },
        "schemaVersion": {
            "type": "integer",
            "title": "Schema Version"
        },
        "acl": {
            "type": "array",
            "title": "ACL",
            "items": {
                "type": "object",
                "properties": {
                    "sid": {
                        "type": "string",
                        "title": "Sid",
                        "default": "SELF"
                    },
                    "permission": {
                        "type": "string",
                        "title": "Permission",
                        "default": "ADMINISTRATE",
                        "enum": ["READ", "WRITE", "ADMINISTRATE", "NONE"]
                    }
                }
            }
        }
    },
    "required": ["schemaId", "relatedResource"]
}